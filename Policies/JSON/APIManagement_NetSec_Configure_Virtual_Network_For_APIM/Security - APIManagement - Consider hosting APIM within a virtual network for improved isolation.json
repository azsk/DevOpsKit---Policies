{
  "mode": "All",
  "policyRule": {
    "if": {
      "allOf": [
        {
          "field": "type",
          "equals": "Microsoft.ApiManagement/service"
        },
        {
          "field": "Microsoft.ApiManagement/service/virtualNetworkType",
          "equals": "None"
        }
      ]
    },
    "then": {
      "effect": "audit"
    }
  },
  "parameters": {}
}