{
  "mode": "All",
  "policyRule": {
    "if": {
      "allOf": [
        {
          "field": "type",
          "equals": "Microsoft.BotService/botServices"
        },
        {
          "field": "Microsoft.BotService/botServices/enabledChannels",
          "like": "*"
        }
      ]
    },
    "then": {
      "effect": "audit"
    }
  },
  "parameters": {}
}